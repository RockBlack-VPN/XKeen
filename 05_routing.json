	"ext:geosite_v2fly.dat:intel",
        "cloudflare-ech.com",
	"rutracker.org",
	"rutracker.net",
	"static.rutracker.cc",
	"lostfilm.tv",
	"themoviedb.org",
	"image.tmdb.org",

        // hdrezka.ag
        "stream.voidboost.cc",
        "hdrezka.ag",

        "whoer.net",
        "2ip.io"
